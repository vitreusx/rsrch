.PHONY: font-awesome tiny-imagenet-200

font-awesome:
	git clone --depth 1 "https://github.com/Rush/Font-Awesome-SVG-PNG" font-awesome

tiny-imagenet-200:
	wget -O tiny-imagenet-200.zip http://cs231n.stanford.edu/tiny-imagenet-200.zip && \
	unzip -qq tiny-imagenet-200.zip && \
	rm tiny-imagenet-200.zip